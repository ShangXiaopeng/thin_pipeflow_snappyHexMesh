FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 5.39e-7;

boundaryField
{
    outlet
    {
        type            calculated;
	value		$internalField;
    }
    "(tubing|outletHemisphere|inletHemisphere)"
    {
        type            calculated;
        value           $internalField;
    }
    inlet
    {
        type            calculated;
	value		$internalField;    
    }
}


