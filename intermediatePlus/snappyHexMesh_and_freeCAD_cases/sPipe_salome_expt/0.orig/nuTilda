FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nuTilda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 4.2e-5;

boundaryField
{
    topPatch
    {
        type            cyclicAMI;
    }

    wall
    {
        type            fixedValue;
        value           uniform 0;

    }

    bottomPatch
    {
        type            cyclicAMI;

    }
}

